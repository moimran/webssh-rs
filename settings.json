{
  "ssh": {
    "connection": {
      "read_timeout_seconds": 30,
      "write_timeout_seconds": 30,
      "timeout_seconds": 60,
      "channel_timeout_seconds": 120,
      "keepalive_seconds": 30,
      "compress": false
    },
    "crypto": {
      "kex_algorithms": "curve25519-sha256,curve25519-sha256@libssh.org,ecdh-sha2-nistp256,ecdh-sha2-nistp384,ecdh-sha2-nistp521,diffie-hellman-group-exchange-sha256,diffie-hellman-group16-sha512,diffie-hellman-group18-sha512,diffie-hellman-group14-sha256,diffie-hellman-group14-sha1,diffie-hellman-group1-sha1",
      "host_key_algorithms": "ssh-ed25519,ecdsa-sha2-nistp256,ecdsa-sha2-nistp384,ecdsa-sha2-nistp521,rsa-sha2-512,rsa-sha2-256,ssh-rsa,ssh-dss",
      "encryption_client_to_server": "chacha20-poly1305@openssh.com,aes256-gcm@openssh.com,aes128-gcm@openssh.com,aes256-ctr,aes192-ctr,aes128-ctr,aes256-cbc,aes192-cbc,aes128-cbc,3des-cbc",
      "encryption_server_to_client": "chacha20-poly1305@openssh.com,aes256-gcm@openssh.com,aes128-gcm@openssh.com,aes256-ctr,aes192-ctr,aes128-ctr,aes256-cbc,aes192-cbc,aes128-cbc,3des-cbc",
      "mac_client_to_server": "hmac-sha2-256-etm@openssh.com,hmac-sha2-512-etm@openssh.com,hmac-sha1-etm@openssh.com,hmac-sha2-256,hmac-sha2-512,hmac-sha1",
      "mac_server_to_client": "hmac-sha2-256-etm@openssh.com,hmac-sha2-512-etm@openssh.com,hmac-sha1-etm@openssh.com,hmac-sha2-256,hmac-sha2-512,hmac-sha1"
    },
    "terminal": {
      "standard_terminal_type": "xterm",
      "linux_terminal_type": "vt100",
      "fallback_terminal_type": "dumb",
      "default_cols": 80,
      "default_rows": 24
    }
  },
  "server": {
    "address": "127.0.0.1",
    "port": 8888,
    "tls_enabled": false,
    "cert_file": null,
    "key_file": null
  }
}
